<%- include('../Student-layout/profile-header.ejs') %>

    	<!-- SIDEBAR -->
	<section id="sidebar">
		<a href="#" class="brand">
			<i class='bx bxs-book'></i>
     
			<span class="text">  Student </span>
		</a>
		<ul class="side-menu top">
			<li >
				<a href="/studentProfile/<%= student._id %>">
					<i class='bx bxs-dashboard' ></i>
					<span class="text">Dashboard</span>
				</a>
           
				
			</li>
			<li >
				<a href="/studentNotes/<%= student._id %>">
					<i class='bx bxs-shopping-bag-alt' ></i>
					<span class="text">Notes</span>
				</a>
			</li>
			<li >
				<a href="/studentAttendence/<%= student._id %>">
					<i class='bx bxs-doughnut-chart' ></i>
					<span class="text">All Attendence</span>
				</a>
			</li>
			<li class="active">
				<a href="/Extra/<%= student._id %>">
					<i class='bx bxs-doughnut-chart' ></i>
					<span class="text">Extra Curriculam</span>
				</a>
			</li>
			<li >
				<a href="/teacherMessage/<%= student._id %>">
					<i class='bx bxs-doughnut-chart' ></i>
					<span class="text">Teachers Message</span>
				</a>
			</li>
			<li>
				<a href="/mentorMessage/<%= student._id %>">
					<i class='bx bxs-doughnut-chart' ></i>
					<span class="text">Mentor Message</span>
				</a>
			</li>
		</ul>
		<ul class="side-menu">
			<!-- <li>
				<a href="#">
					<i class='bx bxs-cog' ></i>
					<span class="text">Settings</span>
				</a>
			</li> -->
			<li>
				<a href="#" class="logout">
					<i class='bx bxs-log-out-circle' ></i>
					<span class="text">Logout</span>
				</a>
			</li>
		</ul>
	</section>
	<!-- SIDEBAR -->



	<!-- CONTENT -->
	<section id="content">
		<!-- NAVBAR -->
		<nav>
		
			<i class='bx bx-menu' ></i>


			<a href="#" class="nav-link">  <%= student.name %>  </a>
			<a href="#" class="profile">
				<img src="<%= student.profile %>"  alt="Teacher">
			</a>
		
			<!-- <dark to light checkbox -->
		

			<input type="checkbox" id="switch-mode" hidden>
			<label for="switch-mode" class="switch-mode"></label>
			
			
		
	</nav>
		<!-- NAVBAR -->

		<!-- MAIN -->
		<main>
            <div class="table-data">
				<div class="order">
					<div class="head">
						<h3>Extra Curriculam</h3>
					</div>
					<table id="myTable">
                        <thead>
                            <tr>
                                <th>Title</th>
                                <th>Details</th>
                                <th>Date</th>
                            </tr>
                        </thead>
                        <tbody>
                            <%
                                if(extra.length>0){
                                    extra.forEach((extras)=>{
                                        %>
                                        <tr>
                                            <td><%= extras.title %></td>
                                            <td><%= extras.detail %></td>
                                            <td><%= extras.date %></td>
                                        </tr>
                                        <%
                                    })
                                }
                            %>
                        </tbody>
                    </table>


				</div>
			</div>

		
		</main>
		<!-- MAIN -->
	</section>
	<!-- CONTENT -->

<%- include('../Student-layout/profile-footer.ejs') %>