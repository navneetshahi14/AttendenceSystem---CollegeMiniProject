<%- include('../Teacher-Layout/notesHeader.ejs') %>

<div class="container">
    <div class="navbar" id="navbar">
        <img id="img" src="<%= Teacher.profile %>" alt="">
        <ul id="details">
            <li>Name: <span><%= Teacher.name %></span></li>
            <li>Email: <span><%= Teacher.email %></span></li>
        </ul>

        <ul class="mainContentbtn">
            <a href="/attendence/<%= Teacher._id %>"><li id="marker"><i class="fa-solid fa-marker " id="markerIcon" ></i><span id="Attendence">Attendence</span></li></a>
            <a href="/notes/<%= Teacher._id %>"><li id="sticky"><i class="fa-regular fa-note-sticky" id="stickyIcon" ></i><span id="notes">Notes</span></li></a>
        </ul>
        
        <div class="logout" id="logoutdiv">
            <a href="#"><button id="button" class="logout-btn"><i class="fa-solid fa-right-from-bracket"></i><span id="logout">Logout</span></button></a>
        </div>
    </div>
    <!-- main dashboard -->
    <div class="dashboard" id="dashboard" >
        <i class="fa-solid fa-bars" id="bars" ></i>

        <div class="notes">
            <div class="notes-modal" id="notesmodal">
                <img src="data:image/jpeg;base64," alt="">
                <h3>Upload</h3>
            </div>
            <%
                if(notes.length > 0){
                    notes.forEach(function(note){
                        %>
                        <div class="uploaded-notes">
                            <div class="notes-modal">
                                <p><%= note.title %></p>
                            </div>
                            <div class="button-notes">
                                <label onclick="EditopenBy('Edit<%= note._id %>')"><i class="fa-solid fa-pen-to-square"></i></label>
                                <label for="btn"><i class="fa-solid fa-trash"></i></label>
                            </div>
                            <form action="/delete-notes" method="post">
                                <input type="hidden" name="teacherId" value="<%= Teacher._id %>">
                                <input type="hidden" name="noteId" value="<%= note._id %>" >
                                <input type="submit" value="Button" id="btn" class="hidden">
                            </form>
                            <div class="EditUpdate hidden" id="Edit<%= note._id %>" >
                                <i class="fa-solid fa-xmark" onclick="EditcloseBy('Edit<%= note._id %>')" ></i>
                                <form action="/update-notes" method="post">
                                    <input type="hidden" name="teacherId" value="<%= Teacher._id %>">
                                    <input type="hidden"  name="noteId" value="<%= note._id %>" >
                                    <input type="text" name="title" value="<%= note.title %>" >
                                    <input type="submit" value="Button">
                                </form>
                            </div>
                        </div>
                        <%
                    })
                }
            %>
        </div>


    </div>
    <div class="notesUploadModal hidden" id="notesUploadModal">
        <div class="formbox">
            <i class="fa-solid fa-xmark" id="notesmodalcross" ></i>
            <form id="titleform" method="post" action="/upload-titled-notes">
                <input type="text" name="title" id="title" placeholder="Notes Title" required>
                <input type="hidden" name="teacherId" value="<%= Teacher._id %>">
                <input type="text" required readonly name="notes" id="note">
                <div id="openPdf" >Upload Pdf Here</div>
                <input type="submit" value="Upload">
            </form>
        </div>
    </div>


    <div class="notesUploadModal hidden" id="pdfupload">
        <div class="formbox">
            <i class="fa-solid fa-xmark" id="notesmodalcrosses" ></i>
            <form id="notesForm">
                <input type="file" name="notes" id="notes" required placeholder="Notes">
                <input type="submit" value="Upload">
            </form>
        </div>
    </div>

</div>

<script>

    $(document).ready(function(){
        $('#notesForm').submit(function(e){
            e.preventDefault()

            $.ajax({
                    url:'/upload-notes',
                    type:"POST",
                    data: new FormData(this),
                    contentType:false,
                    cache:false,
                    processData:false,
                    success:function(data){
                        if(data.success == true){
                            $('#note').val(data.path)
                            alert(data.msg);
                            $("#uploadModal").modal("hide")
                        }else{
                            alert(data.msg);
                        }
                    }
                })
        })

        // $('#titleform').submit(function(e){
        //     e.preventDefault()

        //     let formData =new FormData(this)
        //     console.log(formData)
        //     $.ajax({
        //         url:'/upload-titled-notes',
        //         type:"POST",
        //         data:formData,
        //         contentType:false,
        //         cache:false,
        //         processData:false
        //     })
        // })
    })

</script>


<%- include('../Teacher-Layout/notesFooter.ejs') %>